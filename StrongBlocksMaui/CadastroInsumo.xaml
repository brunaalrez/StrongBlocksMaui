<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StrongBlocksMaui.CadastroInsumo"
             Title="CadastroInsumo">
    <VerticalStackLayout>
        <Label Text="Digite o nome do insumo"/>
        <Entry x:Name="EntryNome"/>
        <Label Text="Digite a quantidade"/>
        <Entry x:Name="EntryQuantidade"/>
        <Label Text="Digite o nome do fornecedor"/>
        <Entry x:Name="EntryFornecedor"/>

        <Button Text="Salvar" Clicked="Salvar_Clicked"/>
        

    </VerticalStackLayout>
</ContentPage>